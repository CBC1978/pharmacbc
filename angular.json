{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pharma": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/pharma",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "https://cdnjs.cloudflare.com/ajax/libs/material-design-icons/4.0.0/css/material-icons.min.css",
              "src/assets/css/vendor.bundle.base.css",
              "src/assets/css/jquery-jvectormap.css",
              "https://cdn.jsdelivr.net/npm/flag-icons@7.0.0/css/flag-icons.min.css",
              "src/assets/css/owl.carousel.min.css",
              "src/assets/css/owl.theme.default.min.css",
              "src/assets/css/style.css"
            ],
            "scripts": [
              "src/assets/js/vendor.bundle.base.js",
              "src/assets/js/Chart.min.js",
              "src/assets/js/progressbar.min.js",
              "src/assets/js/jquery-jvectormap.min.js",
              "src/assets/js/jquery-jvectormap-world-mill-en.js",
              "src/assets/js/owl.carousel.min.js",
              "src/assets/js/jquery.cookie.js",
              "src/assets/js/off-canvas.js",
              "src/assets/js/hoverable-collapse.js",
              "src/assets/js/misc.js",
              "src/assets/js/settings.js",
              "src/assets/js/todolist.js",
              "src/assets/js/dashboard.js"
            ],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "pharma:build:production"
            },
            "development": {
              "buildTarget": "pharma:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "pharma:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
